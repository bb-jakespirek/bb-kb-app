					<span class="label label-success"><a href='https://na14.salesforce.com/articles/Article/{{articleNumber}}' target='_blank'>Open in Salesforce</a></span>
				</li>
			</ul>
		</div>
	{{/if}}
	
{{else}}
	{{#unless help_topic_valid}}
		{{#unless no_kb_necessary}}
